		"@types/jquery": "^3.5.4",
		"@types/mocha": "^8.0.4",
		"@types/node": "^14",
		"@typescript-eslint/eslint-plugin": "^4.8.1",
		"@typescript-eslint/parser": "^4.8.1",
		"chai": "^4.2.0",
		"cross-env": "^7.0.2",
