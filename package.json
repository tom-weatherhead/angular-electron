		"electron-builder": "^22.9.1",
		"eslint": "^7.13.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-rxjs": "^2.1.3",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~6.0.0",
		"karma": "~5.2.3",
		"karma-coverage": "~2.0.3",
